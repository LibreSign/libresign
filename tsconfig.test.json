{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"noEmit": true,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": false
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
		"src/**/*.spec.ts"
	]
}
